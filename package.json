{
  "publisher": "tzyito",
  "name": "vscode-markdown-codesandbox",
  "displayName": "vscode-markdown-codesandbox",
  "version": "0.0.3",
  "packageManager": "pnpm@8.10.5",
  "description": "run code in your markdown",
  "author": "Tzyiro <https://github.com/tzyito>",
  "license": "MIT",
  "homepage": "https://github.com/tzyito/vscode-markdown-codesandbox#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tzyito/vscode-markdown-codesandbox"
  },
  "bugs": {
    "url": "https://github.com/tzyito/vscode-markdown-codesandbox/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/tzyito"
  },
  "keywords": [
    "markdown",
    "vscode"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "public/icon.jpeg",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.84.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies --no-git-checks",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "dependencies": {
    "markdown-it": "^13.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
